{
  "name": "vite-lib-starter",
  "version": "0.5.0",
  "upperName": "ViteLibStarter",
  "description": "vite-lib-starter",
  "author": "SoulLyoko <519052532@qq.com>",
  "repository": "https://github.com/SoulLyoko/vite-lib-starter.git",
  "homepage": "https://github.com/SoulLyoko/vite-lib-starter#readme",
  "private": false,
  "license": "MIT",
  "keywords": [
    "vite",
    "lib",
    "starter"
  ],
  "files": [
    "dist",
    "es",
    "lib",
    "global.d.ts"
  ],
  "main": "lib/index.js",
  "module": "es/index.js",
  "types": "es/index.d.ts",
  "unpkg": "dist/index.js",
  "jsdelivr": "dist/index.js",
  "style": "dist/index.css",
  "exports": {
    ".": {
      "import": "./es/index.js",
      "require": "./lib/index.js"
    },
    "./*": "./*"
  },
  "scripts": {
    "dev": "yarn switch:3 && vite",
    "dev:2": "yarn switch:2 && vite",
    "dev:2.7": "yarn switch:2.7 && vite",
    "build": "yarn switch:3 && yarn typecheck && vite build",
    "build:global": "esno scripts/global.ts",
    "postbuild": "esno scripts/postbuild.ts",
    "serve": "yarn switch:3 && vite preview",
    "docs:dev": "yarn switch:3 && vitepress dev packages",
    "docs:build": "yarn switch:3 && vitepress build packages",
    "docs:serve": "yarn switch:3 && vitepress serve packages",
    "test": "vitest",
    "test:2": "yarn switch:2 && vitest run",
    "test:2.7": "yarn switch:2.7 && vitest run",
    "test:3": "yarn switch:3 && vitest run",
    "test:all": "yarn test:2 && yarn test:2.7 && yarn test:3",
    "test:coverage": "vitest --coverage",
    "switch:2": "esno scripts/switch.ts 2",
    "switch:2.7": "esno scripts/switch.ts 2.7",
    "switch:3": "esno scripts/switch.ts 3",
    "release": "esno scripts/release.ts",
    "typecheck": "vue-tsc --noEmit",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "ci": "vite build && yarn docs:build && yarn test:all",
    "prepare": "husky install",
    "lint": "eslint --ext .ts,.vue ."
  },
  "dependencies": {
    "eslint-plugin-import": "^2.26.0",
    "vue-demi": "^0.13.8"
  },
  "peerDependencies": {
    "@vue/composition-api": "^1.7.0",
    "vue": "^2.6.0 || ^3.2.0"
  },
  "peerDependenciesMeta": {
    "@vue/composition-api": {
      "optional": true
    }
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@types/fs-extra": "^9.0.13",
    "@types/gulp": "^4.0.9",
    "@types/node": "^18.7.9",
    "@typescript-eslint/eslint-plugin": "^5.33.1",
    "@typescript-eslint/parser": "^5.33.1",
    "@vitejs/plugin-vue": "^2.3.4",
    "@vitejs/plugin-vue2": "^1.1.2",
    "@vue/compiler-sfc": "^3.2.37",
    "@vue/composition-api": "^1.7.0",
    "@vue/test-utils": "^2.0.2",
    "@vue/tsconfig": "^0.1.3",
    "c8": "^7.12.0",
    "conventional-changelog-cli": "^2.2.2",
    "enquirer": "^2.3.6",
    "eslint": "^8.22.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.3.0",
    "esno": "^0.16.3",
    "fs-extra": "^10.1.0",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^8.0.0",
    "gulp-clean-css": "^4.3.0",
    "gulp-sass": "^5.1.0",
    "husky": "^8.0.1",
    "jsdom": "^20.0.0",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "sass": "^1.54.5",
    "tsc-alias": "^1.7.0",
    "typescript": "^4.7.4",
    "vite": "^2.9.15",
    "vite-plugin-vue2": "^2.0.2",
    "vitepress": "^0.22.4",
    "vitest": "^0.22.1",
    "vue": "^3.2.37",
    "vue-template-compiler": "^2.7.9",
    "vue-tsc": "^0.40.1"
  },
  "lint-staged": {
    "*.{ts,vue}": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
}
