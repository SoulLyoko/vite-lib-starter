{
  "name": "vite-lib-starter",
  "version": "0.3.2",
  "description": "vite-lib-starter",
  "author": "SoulLyoko <519052532@qq.com>",
  "repository": "https://github.com/SoulLyoko/vite-lib-starter.git",
  "homepage": "https://github.com/SoulLyoko/vite-lib-starter#readme",
  "private": false,
  "license": "MIT",
  "files": [
    "lib",
    "global.d.ts"
  ],
  "main": "lib/index.umd.js",
  "module": "lib/index.es.js",
  "types": "lib/index.d.ts",
  "keywords": [
    "vite",
    "lib",
    "starter"
  ],
  "scripts": {
    "dev": "yarn switch:3 && vite",
    "dev:2": "yarn switch:2 && vite",
    "build": "yarn switch:3 && vite build && yarn build:types",
    "build:types": "vue-tsc --project tsconfig.d.json --declaration --emitDeclarationOnly --outDir lib",
    "serve": "yarn switch:3 && vite preview",
    "docs:dev": "yarn switch:3 && vitepress dev packages",
    "docs:build": "yarn switch:3 && vitepress build packages",
    "docs:serve": "yarn switch:3 && vitepress serve packages",
    "test": "vitest",
    "test:2": "yarn switch:2 && vitest run",
    "test:3": "yarn switch:3 && vitest run",
    "test:all": "yarn test:2 && yarn test:3",
    "test:coverage": "vitest --coverage",
    "switch:2": "esno scripts/switch.ts 2",
    "switch:3": "esno scripts/switch.ts 3",
    "release": "esno scripts/release.ts",
    "typecheck": "vue-tsc --noEmit",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "ci": "yarn typecheck && yarn test:all && yarn build && yarn docs:build",
    "prepare": "husky install",
    "lint": "eslint --ext .ts,.vue ."
  },
  "dependencies": {
    "vue-demi": "^0.12.5"
  },
  "peerDependencies": {
    "@vue/composition-api": "^1.6.0",
    "vue": "^2.6.0 || ^3.2.0"
  },
  "peerDependenciesMeta": {
    "@vue/composition-api": {
      "optional": true
    }
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.4",
    "@commitlint/config-conventional": "^16.2.4",
    "@types/node": "^17.0.32",
    "@typescript-eslint/eslint-plugin": "^5.22.0",
    "@typescript-eslint/parser": "^5.22.0",
    "@vitejs/plugin-vue": "^2.3.3",
    "@vue/compiler-sfc": "^3.2.33",
    "@vue/composition-api": "^1.6.0",
    "@vue/test-utils": "^2.0.0-rc.21",
    "@vue/tsconfig": "^0.1.3",
    "c8": "^7.11.2",
    "conventional-changelog-cli": "^2.2.2",
    "enquirer": "^2.3.6",
    "eslint": "^8.15.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.7.1",
    "esno": "^0.14.1",
    "husky": "^8.0.1",
    "jsdom": "^19.0.0",
    "lint-staged": "^12.4.1",
    "prettier": "^2.6.2",
    "typescript": "^4.6.4",
    "vite": "^2.9.8",
    "vite-plugin-vue2": "^2.0.0",
    "vitepress": "^0.22.4",
    "vitest": "^0.12.4",
    "vue": "^3.2.33",
    "vue-template-compiler": "^2.6.14",
    "vue-tsc": "^0.34.13"
  },
  "lint-staged": {
    "*.{ts,vue}": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
}
